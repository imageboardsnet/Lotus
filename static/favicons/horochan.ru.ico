	<script>
		const prefersDarkScheme = () => window.matchMedia("(prefers-color-scheme: dark)").matches;
		const getThemeName = (isDark) => isDark ? 'dark' : 'light'
		const setColorScheme = (colorScheme) => document.firstElementChild.setAttribute("color-scheme", colorScheme);

		function setTheme() {
			// If we have stored the user's preferences we will prefer that
			const preferencesJson = localStorage.getItem("themes");
			if (preferencesJson) {
				const preferences = JSON.parse(preferencesJson);
				if (preferences.currentTheme) {
					// Dark/Light
					if (preferences.currentTheme != 'system') {
						setColorScheme(preferences.currentTheme)
					}
					// System theme
					else {
						setColorScheme(getThemeName(prefersDarkScheme()))
					}
				}
			}
			// Otherwise, we check their OS settings
			else {
				setColorScheme(getThemeName(prefersDarkScheme()))
			}
		}

		// Sets the theme as early as possible to avoid a flash of incorrect theme
		setTheme();

		document.addEventListener("DOMContentLoaded", () => setTheme());
	</script>
